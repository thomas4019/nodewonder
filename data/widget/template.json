{
    "slots": [
        "body"
    ],
    "wrapper": "none",
    "settingsModel": [
        {
            "name": "path",
            "type": "Text"
        }
    ],
    "load": {
        "_is_func": true,
        "args": [
            "callback"
        ],
        "javascript": "fs.readFile(this.settings.path, 'utf8', function(err, data) {\n   this.template = Handlebars.compile(data); callback();\n}.bind(this));"
    },
    "toHTML": {
        "_is_func": true,
        "args": [
            "label"
        ],
        "javascript": "return this.template(this);"
    },
    "name": "template",
    "tags": [
        "view"
    ],
    "settings_unfiltered": []
}