{
    "slots": [
        "body"
    ],
    "wrapper": "none",
    "load": {
        "_is_func": true,
        "args": [
            "callback"
        ],
        "javascript": "fs.readFile(this.settings.path, 'utf8', function(err, data) {\n   this.template = Handlebars.compile(data); callback();\n}.bind(this));"
    },
    "toHTML": {
        "_is_func": true,
        "args": [
            "label"
        ],
        "javascript": "return this.template(this);"
    },
    "name": "template",
    "tags": [
        "view"
    ],
    "settings_unfiltered": [],
    "settings": {
        "fields": {
            "path": {
                "type": "Text"
            }
        },
        "display": {
            "widgets": {
                "path": {
                    "type": "textbox",
                    "slots": {},
                    "settings": {
                        "label": "path",
                        "field_type": "Text"
                    },
                    "field": "path",
                    "model_type": "Text",
                    "model": "template"
                }
            },
            "slotAssignments": {
                "body": [
                    "path"
                ]
            }
        }
    }
}