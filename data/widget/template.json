{
    "slots": [
        "body"
    ],
    "wrapper": "none",
    "settingsModel": [
        {
            "name": "path",
            "type": "Text"
        }
    ],
    "load": {
        "_is_func": true,
        "args": [
            "callback"
        ],
        "javascript": "var that = this;\nfs.readFile(this.settings.path, 'utf8', function(err, data) {\n    if (err) {\n        return console.log(err);\n    }\n    that.template = Handlebars.compile(data);\n    callback();\n});"
    },
    "toHTML": {
        "_is_func": true,
        "args": [
            "label"
        ],
        "javascript": "return this.template(this);"
    },
    "name": "template",
    "tags": [
        "view"
    ],
    "settings_unfiltered": []
}