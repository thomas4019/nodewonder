{
    "tags": [
        "field_edit",
        "view"
    ],
    "deps": {
        "jquery": [],
        "bootstrap": []
    },
    "toHTML": {
        "_is_func": true,
        "args": [
            "label"
        ],
        "javascript": "var element = '<input class=\"form-control input-small\" type=\"password\" name=\"' + this.id + '\"' + (this.settings.data ? ' value=\"' + cms.functions.htmlEscape(this.settings.data) + '\"' : '') + ' />';\n\nif (this.settings.inline) {\n    return '<div class=\"controls form-inline\">' + (this.settings.label ? label : '') + element + '</div>';\n} else {\n    return (this.settings.label ? label : '') + element;\n}"
    },
    "name": "password",
    "settings_unfiltered": [
        "data",
        "data",
        "data",
        "data",
        "data"
    ],
    "settings": {
        "fields": {
            "inline": {
                "type": "Boolean"
            },
            "data": {
                "type": "Text"
            }
        },
        "display": {
            "widgets": {
                "inline": {
                    "type": "checkbox",
                    "slots": {},
                    "settings": {
                        "label": "inline",
                        "field_type": "Boolean"
                    },
                    "field": "inline",
                    "model_type": "Boolean",
                    "model": "password"
                },
                "data": {
                    "type": "textbox",
                    "slots": {},
                    "settings": {
                        "label": "data",
                        "field_type": "Text"
                    },
                    "field": "data",
                    "model_type": "Text",
                    "model": "password"
                }
            },
            "slotAssignments": {
                "body": [
                    "inline",
                    "data"
                ]
            }
        }
    }
}