{
    "name": "ckeditor",
    "settings_unfiltered": [
        "data"
    ],
    "htmlTemplate": {
        "_is_func": true,
        "args": [],
        "htmlmixed": "{{{label}}}\n<textarea id=\"{{id}}-Editor\" name=\"{{id}}\">{{settings.data}}</textarea>"
    },
    "deps": {
        "jquery": [],
        "ckeditor": [
            "ckeditor.js"
        ]
    },
    "head": [],
    "tags": [
        "field_edit",
        "view",
        "action",
        "local-action"
    ],
    "slots": {},
    "wrapper": "",
    "wrapperClass": "",
    "action": {
        "_is_func": true,
        "args": [
            "settings",
            "id",
            "scope",
            "handlers"
        ],
        "javascript": "CKEDITOR.replace(id + '-Editor',{toolbar:\"Basic\"});"
    },
    "weight": null,
    "settings": {
        "fields": {
            "label": {
                "type": "Text",
                "quantity": ""
            },
            "toolbar": {
                "type": "Text",
                "quantity": ""
            },
            "data": {
                "type": "Text",
                "quantity": ""
            }
        },
        "display": {
            "widgets": {
                "label": {
                    "type": "ckeditor",
                    "slots": {},
                    "settings": {
                        "label": "label",
                        "field_type": "Text"
                    },
                    "field": "label",
                    "model_type": "Text",
                    "model": "ckeditor"
                },
                "toolbar": {
                    "type": "select",
                    "slots": {},
                    "settings": {
                        "label": "toolbar",
                        "choices": [
                            "Basic",
                            "Advanced"
                        ],
                        "field_type": "Text"
                    },
                    "field": "toolbar",
                    "model_type": "Text",
                    "model": "ckeditor"
                },
                "data": {
                    "type": "ckeditor",
                    "slots": {},
                    "settings": {
                        "label": "data",
                        "field_type": "Text"
                    },
                    "field": "data",
                    "model_type": "Text",
                    "model": "ckeditor"
                }
            },
            "slotAssignments": {
                "body": [
                    "label",
                    "toolbar",
                    "data"
                ]
            }
        }
    }
}