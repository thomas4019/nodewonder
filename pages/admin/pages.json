{
    "slotAssignments": {
        "body": [
            "start"
        ]
    },
    "widgets": {
        "start": {
            "path": "admin.html",
            "slots": {
                "body": [
                    "eODpMrEO",
                    "listing",
                    "create",
                    "ypxzmTrO",
                    "Cp5smAya"
                ],
                "top": [
                    "menu"
                ]
            },
            "type": "template"
        },
        "menu": {
            "path": "menu.html",
            "type": "htmlfile"
        },
        "create": {
            "label": "Add a new page",
            "type": "submit"
        },
        "Cp5smAya": {
            "type": "page_listing"
        },
        "eODpMrEO": {
            "label": "URL",
            "type": "field_text",
            "name": "page_url"
        }
    },
    "rules": [
        {
            "event": {
                "clicked": {
                    "sel": "#submit input"
                }
            },
            "actions": [
                {
                    "execute": {
                        "js": "var page = $('input[name=\"page_url\"]').val(); window.location = '/admin/pages/edit?storage-file='+page+'&editor-page='+page+'&preview-url=/'+page;"
                    }
                }
            ]
        }
    ]
}